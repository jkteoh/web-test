{
  "name": "@moneylion/mlds-web",
  "repository": {
    "type": "git",
    "url": "https://github.com/MoneyLion/mlds-web.git"
  },
  "version": "2.0.1",
  "description": "MoneyLion Design System - WEB",
  "main": "index.js",
  "author": "jteoh <jteoh@moneylion.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn build:package false",
    "build-storybook": "build-storybook -o docs",
    "build:prod": "yarn build:package true",
    "build:package": "f() { cross-env TS_NODE_PROJECT=\"build/webpack.tsconfig.json\" IS_RELEASE=$1 webpack --progress --config build/webpack.config.ts ;}; f",
    "generateComponent": "ts-node ./scripts/generateComponent",
    "generateIconFonts": "ts-node ./scripts/generateIconFonts.ts",
    "prepare": "husky install; ts-patch install",
    "prettier": "prettier --write src",
    "storybook": "start-storybook -p 6006",
    "test": "yarn test:eslint && yarn test:jest",
    "test:eslint": "eslint src --ext .ts,.tsx",
    "test:jest": "jest --config ./jest.config.json --verbose",
    "test:typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.7",
    "classnames": "^2.3.1",
    "is-hotkey": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@moneylion/eslint-config-fe": "^1.0.4",
    "@moneylion/prettier-config-fe": "^2.0.0",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/builder-webpack5": "^6.3.1",
    "@storybook/manager-webpack5": "^6.3.1",
    "@storybook/react": "^6.3.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.0.7",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/jest": "^26.0.21",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dts-bundle-generator": "^5.9.0",
    "eslint": "^7.27.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "^6.0.0",
    "inquirer": "^8.1.0",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "prettier": "2.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "spinnies": "0.4.2",
    "style-loader": "^3.3.1",
    "styled-components": "^5.2.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-patch": "^1.3.3",
    "typescript": "4.2.3",
    "typescript-transform-paths": "^2.2.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --write"
  },
  "types": "index.d.ts"
}